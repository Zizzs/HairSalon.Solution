@using HairSalon.Models
@{
  Layout = "_Layout";
}

<div class="container">
    <h3>Hair Salon</h3>
    <h2>@Model["stylists"][0].GetName()</h2>
    <h3>Current Clients:</h3>
    @foreach(ClientClass client in @Model["clients"])
    {
        <p>@Model["clients"][0].GetName()</p>
    }
    <br>
    <a href="/stylists">Home Page</a>
</div>
